{
  "version": 2,
  "builds": [
    {
      "src": "index-modular.html",
      "use": "@vercel/static"
    },
    {
      "src": "test-modules.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*.json",
      "use": "@vercel/static"
    },
    {
      "src": "tests/**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index-modular.html"
    },
    {
      "src": "/test",
      "dest": "/test-modules.html"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/tests/(.*)",
      "dest": "/tests/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/src/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/src/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}